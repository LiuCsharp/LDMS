  <data name="tileView1.TileHtmlTemplate.Styles" xml:space="preserve">
    <value>.root {
	height: 58px;
	font-family: "Segoe UI";
	font-size: 14px;
	background-color: Transparent;
	border-style: solid;
	border-width: 0px;
	border-left-width: 4px;
	border-left-color: Transparent;
	
}
.root:hover{
	background-color:@HighlightAlternate/0.1;
}

.FileDate {
	font-size: 1px;
}

.content {
	margin-top:18%;
	margin-bottom:0px;
	margin-Left:16px;
	margin-right:20px;
	display:flex;
	background-color:Transparent;
}
.initials {
	color: @White;
	font-weight: bold;
	background-color: @Primary;
	border-radius: 100px;
	width: 32px;
	height: 32px;
	display: flex;
	align-items: center;
	justify-content: center;
}
.photo {
	width:33px;
	height:33px;
	border-radius:100px;
	border: none;
	object-fit:contain;
	background-color:Transparent ;
	padding-left: -15px;
}
.mail-content {
	flex-grow: 1;
	margin-left: 8px;
	
}
.plaintext {
	text-overflow:ellipsis;
	overflow:hidden;
	white-space:nowrap;
}
.date {
	align-self:center;
	text-align:left;
	width: 150px;
	padding-right: -30px;
}
.from {
	text-overflow:ellipsis;
	overflow:hidden;
	white-space:nowrap;
	margin-top:-7px;
	font-weight:normal;
	font-size: 17px;
	
}
.bold-text {
	font-weight:bold;
	font-size: 20px;
}
.subject {
	text-overflow:ellipsis;
	overflow:hidden;
	white-space:nowrap;
}
.unread-stripe {
	border-left-color: @Primary;
}
.unread-color {
	color: @Primary;
}
.hidden {
	display:none;
	border: none;
}
.buttons {
	margin-right:-6px;
	display:flex;
	flex-wrap:nowrap;
	justify-content:flex-end;
}

.btn {
	width:18px;
	height:18px;
	min-width:18px;
	object-fit:contain;
	opacity:0;
}
.opaque { opacity:1; }
.root:hover .btn { opacity: 0.5; }
.root .btn:hover { opacity: 1; }
</value>
  </data>
  <data name="tileView1.TileHtmlTemplate.Template" xml:space="preserve">
    <value>&lt;div class="root" dx-class="{IsUnread:unread-stripe}"&gt;
	&lt;div class="content"&gt;
		&lt;div dx-class="{HasPhoto:,hidden}"&gt;&lt;img class="photo" src="${FileImage}"/&gt;&lt;/div&gt;
		&lt;div class="mail-content"&gt;
			&lt;div class="from" id="from" dx-class="{IsUnread:bold-text}"&gt;${FileName}&lt;/div&gt;
			&lt;div class="subject" dx-class="{IsUnread:bold-text unread-color}"&gt;${FilePath}&lt;/div&gt;
			&lt;div class="FileDate" &gt;${FileDate}&lt;/div&gt;	
		&lt;/div&gt;
		&lt;div&gt;
			&lt;div class="buttons"&gt;
				&lt;img class="btn" title="从列表中移除" src="clearheaderandfooter" onclick="OnDelete"/&gt;
			&lt;/div&gt;
			&lt;div class="date" dx-class="{IsUnread:unread-color}"&gt;${FileTime}&lt;/div&gt;
		&lt;/div&gt;
	&lt;/div&gt;
&lt;/div&gt;</value>
  </data>
