  <data name="htmlContentPopup1.HtmlTemplate.Styles" xml:space="preserve">
    <value>.container {
	display: flex;
	flex-direction: column;
	align-items: center;
	padding: 8px 16px 24px 16px;
}

.shadow {
	border-color: lightgray;
	border-width: 1px;
	border-style: solid;
	border-radius: 8px;
	box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.1);
	font-size:18px;
	background-color: @Window;
	color: @WindowText;
	padding:16px;
}
</value>
